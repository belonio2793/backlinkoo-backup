  "discoveryMethods": {
    "search_engines": {
      "queries": [
        "\"submit your site\" \"add url\" inurl:submit",
        "\"create account\" \"publish\" \"blog\" site:*.wordpress.com",
        "\"guest post\" \"submit article\" \"author bio\"",
        "\"directory submission\" \"add website\" \"free listing\"",
        "\"profile creation\" \"about me\" \"portfolio\" high DA",
        "\"forum\" \"signature\" \"member profile\" inurl:profile",
        "\"comment\" \"discussion\" \"leave reply\" high DA blog",
        "\"submit story\" \"user content\" \"publish article\"",
        "\"social bookmarking\" \"share link\" \"add bookmark\"",
        "\"wiki\" \"edit\" \"contribute\" \"add content\"",
        "\"Q&A\" \"ask question\" \"answer\" \"help community\"",
        "\"document sharing\" \"upload file\" \"publish document\""
      ]
    },
    "competitor_analysis": {
      "methods": [
        "ahrefs_backlinks",
        "semrush_backlinks", 
        "moz_link_explorer",
        "majestic_backlinks"
      ]
    },
    "automation_discovery": {
      "form_selectors": [
        "form[action*='submit']",
        "form[action*='register']",
        "form[action*='signup']",
        "form[action*='comment']",
        "form[action*='contact']",
        "input[name*='url']",
        "input[name*='website']",
        "input[name*='link']",
        "textarea[name*='description']",
        "input[name*='title']"
      ]
    }
  },

